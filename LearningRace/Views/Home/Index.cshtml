@model LR.Models.Category
@section customScripts{
    <script src="../../Scripts/Cutegories/CategoriesPicker.js" type="text/javascript"></script>
}

@section featured {
    @Model.Name
}

@{var childViewData = new ViewDataDictionary(new ViewDataDictionary {{ "IsAdmin", ViewBag.IsAdmin }});}
@if (Model.ChildCategories != null)
{
@Html.Partial("CategoryPicker", Model.ChildCategories as object, childViewData)
}